#include <stdio.h>
int main() 
{
    int time = 0;
    scanf("%d", &time);
    int total = 10+5*time;
    int totalExceeds53 = total > 53;
    if(totalExceeds53)
    {
        printf("53");
    }else
    {
        printf("%d", total);
    }
    return 0;
}

/*50% score answer*/

#include <stdio.h>
int main()
{
    int i, price, pay;
    i = 7;
    price = 10;
    if((i <= 0) || (i == 0))
    {
    printf("%d", price); 
    }
    else((i >= 0) || (i != 0));
    {
    pay = (i*5)+10; 
    printf("%d", pay, (i*5)+10);
    ++i;
    }
    return 0;
}
